{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "types": ["node"],
    "typeRoots": ["./node_modules/@types"],
    "module": "nodenext",
    "target": "es2022",
    "noEmit": false,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "paths": {
      "@env": ["./src/env"]
    }
  }
}
