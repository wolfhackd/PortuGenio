{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "types": ["node"],
    "module": "nodenext",
    "target": "es2022",
    "noEmit": false,
    "outDir": "dist",
    // "allowImportingTsExtensions": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "@env": ["./src/env"]
    }
  }
}
